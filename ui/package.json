  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "build": "ng build && stylus src/assets/styles/signin.styl --out ../public",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "ng serve --open --proxy-config proxy.conf.json --host 0.0.0.0 --output-hashing none && stylus src/assets/styles/signin.styl --out ../public",
    "test-no-watch": "ng test --single-run",
    "build-dev": "ng build --progress --output-path ../public --output-hashing none && stylus src/assets/styles/signin.styl --out ../public",
    "build-prod": "ng build --stats-json --extract-css --progress --prod --aot --output-path ../public --output-hashing none && stylus src/assets/styles/signin.styl --out ../public",
    "bundle-report": "webpack-bundle-analyzer ../public/stats.json"
  },
  "dependencies": {
    "@angular-devkit/core": "^0.5.6",
    "@angular/animations": "^5.0.0",
